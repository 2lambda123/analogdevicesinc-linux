/dts-v1/;
#include "socfpga_arria10.dtsi"

/ {
	model = "Altera SOCFPGA Arria 10 iW-G24D platform";
	compatible = "altr,socfpga-arria10", "altr,socfpga";

	chosen {
		bootargs = "earlyprintk";
		stdout-path = "serial0:115200n8";
	};

	memory {
		name = "memory";
		device_type = "memory";
		reg = <0x0 0x40000000>; /* 1GB */
	};

	aliases {
		ethernet0 = &gmac1;
		serial0 = &uart0;
	};

	clocks {
		sys_clk: sys_clk {
			#clock-cells = <0>;
			compatible = "fixed-clock";
			clock-frequency = <100000000>;
			clock-output-names = "system_clock";
		};

		dma_clk: dma_clk {
			#clock-cells = <0>;
			compatible = "fixed-clock";
			clock-frequency = <166666667>;
			clock-output-names = "dma_clock";
		};
	};

	soc {
		clkmgr@ffd04000 {
			clocks {
				osc1 {
					clock-frequency = <25000000>;
				};
			};
		};

		sys_hps_bridges: bridge@ff200000 {
			compatible = "simple-bus";
			reg = <0xff200000 0x00200000>;
			#address-cells = <2>;
			#size-cells = <1>;
			ranges = <0x00000001 0x00000000 0xff200000 0x00000010>,
				<0x00000001 0x00000020 0xff200020 0x00000010>,
				<0x00000001 0x00000040 0xff200040 0x00000020>,
				<0x00000001 0x00020000 0xff220000 0x00004000>,
				<0x00000001 0x00024000 0xff224000 0x00001000>,
				<0x00000001 0x00025000 0xff225000 0x00001000>,
				<0x00000001 0x00026000 0xff226000 0x00001000>,
				<0x00000001 0x00028000 0xff228000 0x00001000>,
				<0x00000001 0x00029000 0xff229000 0x00001000>,
				<0x00000001 0x0002a000 0xff22a000 0x00001000>,
				<0x00000001 0x0002b000 0xff22b000 0x00001000>,
				<0x00000001 0x0002c000 0xff22c000 0x00001000>,
				<0x00000001 0x00030000 0xff230000 0x00010000>,
				<0x00000001 0x00040000 0xff240000 0x00004000>,
				<0x00000001 0x00044000 0xff244000 0x00001000>,
				<0x00000001 0x00045000 0xff245000 0x00001000>,
				<0x00000001 0x00048000 0xff248000 0x00001000>,
				<0x00000001 0x00049000 0xff249000 0x00001000>,
				<0x00000001 0x0004a000 0xff24a000 0x00001000>,
				<0x00000001 0x0004b000 0xff24b000 0x00001000>,
				<0x00000001 0x0004c000 0xff24c000 0x00001000>,
				<0x00000001 0x00050000 0xff250000 0x00010000>;

			sys_gpio_in: gpio@100000000 {
				compatible = "altr,pio-1.0";
				reg = <0x00000001 0x00000000 0x00000010>;
				interrupt-parent = <&intc>;
				interrupts = <0 24 4>;
				altr,gpio-bank-width = <32>;
				altr,interrupt-type = <4>;
				altr,interrupt_type = <4>;
				level_trigger = <1>;
				resetvalue = <0>;
				#gpio-cells = <2>;
				gpio-controller;
			};

			sys_gpio_out: gpio@100000020 {
				compatible = "altr,pio-1.0";
				reg = <0x00000001 0x00000020 0x00000010>;
				altr,gpio-bank-width = <32>;
				resetvalue = <0>;
				#gpio-cells = <2>;
				gpio-controller;
			};

			sys_spi: spi@100000040 {
				compatible = "altr,spi-1.0";
				reg = <0x00000001 0x00000040 0x00000020>;
				interrupt-parent = <&intc>;
				interrupts = <0 26 4>;
				#address-cells = <0x1>;
				#size-cells = <0x0>;
			};

			tx_device_clk_pll: altera-a10-fpll@100025000 {
				compatible = "altr,a10-fpll";
				reg = <0x00000001 0x00025000 0x1000>;
				#clock-cells = <0>;
				clocks = <&clk0_ad9523 9>;
				clock-output-names = "jesd204_tx_link_clock";
			};

			axi_ad9144_jesd: axi-jesd204-tx@100020000 {
				compatible = "adi,axi-jesd204-tx-1.0";
				reg = <0x00000001 0x00020000 0x00004000>;
				interrupt-parent = <&intc>;
				interrupts = <0 28 0>;

				clocks = <&sys_clk>, <&tx_device_clk_pll>, <&axi_ad9144_xcvr>;
				clock-names = "s_axi_aclk", "device_clk", "lane_clk";

				adi,octets-per-frame = <1>;
				adi,frames-per-multiframe = <32>;
				adi,converter-resolution = <16>;
				adi,bits-per-sample = <16>;
				adi,converters-per-device = <2>;

				#clock-cells = <0>;
				clock-output-names = "jesd_dac_lane_clk";
			};

			axi_ad9144_xcvr: axi-ad9144-xcvr@100024000 {
				compatible = "adi,altera-adxcvr-1.00.a";
				reg = <0x00000001 0x00024000 0x00001000>,
				      <0x00000001 0x00026000 0x00001000>,
				      <0x00000001 0x00028000 0x00001000>,
				      <0x00000001 0x00029000 0x00001000>,
				      <0x00000001 0x0002a000 0x00001000>,
				      <0x00000001 0x0002b000 0x00001000>;
				reg-names = "adxcvr", "atx-pll", "adxcfg-0", "adxcfg-1", "adxcfg-2", "adxcfg-3";

				#clock-cells = <0>;
				clocks = <&clk0_ad9523 9>, <&tx_device_clk_pll>;
				clock-names = "ref", "link";
				clock-output-names = "jesd204_tx_lane_clock";
			};

			tx_dma: tx-dmac@10002c000 {
				compatible = "adi,axi-dmac-1.00.a";
				reg = <0x00000001 0x0002c000 0x00001000>;
				interrupt-parent = <&intc>;
				interrupts = <0 30 4>;
				#dma-cells = <1>;
				clocks = <&dma_clk>;

				dma-channel {
					adi,source-bus-width = <128>;
					adi,destination-bus-width = <128>;
					#adi,type = <1>;
					adi,cyclic;
				};
			};

			axi_ad9144_core: axi-ad9144-hpc@100034000 {
				compatible = "adi,axi-ad9144-1.0";
				reg = <0x00000001 0x00034000 0x00004000>;
				dmas = <&tx_dma 0>;
				dma-names = "tx";
				spibus-connected = <&dac0_ad9144>;
				adi,axi-pl-fifo-enable;
			};

			rx_device_clk_pll: altera-a10-fpll@100045000 {
				compatible = "altr,a10-fpll";
				reg = <0x00000001 0x00045000 0x1000>;
				#clock-cells = <0>;
				clocks = <&clk0_ad9523 4>;
				clock-output-names = "jesd204_rx_link_clock";
			};

			axi_ad9680_jesd: axi-jesd204-rx@100040000 {
				compatible = "adi,axi-jesd204-rx-1.0";
				reg = <0x00000001 0x00040000 0x00004000>;
				interrupt-parent = <&intc>;
				interrupts = <0 27 0>;

				clocks = <&sys_clk>, <&rx_device_clk_pll>, <&axi_ad9680_xcvr>;
				clock-names = "s_axi_aclk", "device_clk", "lane_clk";

				adi,octets-per-frame = <1>;
				adi,frames-per-multiframe = <32>;

				#clock-cells = <0>;
				clock-output-names = "jesd_adc_lane_clk";
			};

			axi_ad9680_xcvr: axi-ad9680-xcvr@100044000 {
				compatible = "adi,altera-adxcvr-1.00.a";
				reg = <0x00000001 0x00044000 0x00001000>,
				      <0x00000001 0x00048000 0x00001000>,
				      <0x00000001 0x00049000 0x00001000>,
				      <0x00000001 0x0004a000 0x00001000>,
				      <0x00000001 0x0004b000 0x00001000>;
				reg-names = "adxcvr", "adxcfg-0", "adxcfg-1", "adxcfg-2", "adxcfg-3";

				#clock-cells = <0>;
				clocks = <&clk0_ad9523 4>, <&rx_device_clk_pll>;
				clock-names = "ref", "link";
				clock-output-names = "jesd204_rx_lane_clock";
			};

			rx_dma: rx-dmac@10004c000 {
				compatible = "adi,axi-dmac-1.00.a";
				reg = <0x00000001 0x0004c000 0x00001000>;
				interrupt-parent = <&intc>;
				interrupts = <0 29 4>;
				#dma-cells = <1>;
				clocks = <&dma_clk>;

				dma-channel {
					adi,source-bus-width = <128>;
					adi,destination-bus-width = <128>;
					adi,type = <0>;
				};
			};

			axi_ad9680_core: axi-ad9680-hpc@100050000 {
				compatible = "adi,axi-ad9680-1.0";
				reg = <0x00000001 0x00050000 0x00010000>;
				dmas = <&rx_dma 0>;
				dma-names = "rx";
				spibus-connected = <&adc0_ad9680>;
			};
		};
	};
};

&uart0 {
	status = "okay";
};

&mmc {
	status = "okay";
	num-slots = <1>;
	cap-sd-highspeed;
	broken-cd;
	bus-width = <4>;
	altr,dw-mshc-ciu-div = <3>;
	altr,dw-mshc-sdr-timing = <0 3>;
};

&gmac1 {
	status = "okay";

	phy-mode = "rgmii";
	phy-addr = <0xffffffff>;

	txd0-skew-ps = <0>;	/* -420ps */
	txd1-skew-ps = <0>;	/* -420ps */
	txd2-skew-ps = <0>;	/* -420ps */
	txd3-skew-ps = <0>;	/* -420ps */
	rxd0-skew-ps = <420>;	/* 0ps */
	rxd1-skew-ps = <420>;	/* 0ps */
	rxd2-skew-ps = <420>;	/* 0ps */
	rxd3-skew-ps = <420>;	/* 0ps */
	txen-skew-ps = <0>;	/* -420ps */
	txc-skew-ps = <1860>;	/* 960ps */
	rxdv-skew-ps = <420>;	/* 0ps */
	rxc-skew-ps = <1680>;	/* 780ps */

	max-frame-size = <3800>;
};

&i2c1 {
	status = "okay";
	speed-mode = <0x0>;

	ad7291@2c {
		compatible = "adi,ad7291";
		reg = <0x2c>;
	};
};

#define fmc_spi sys_spi

#include "adi-daq2.dtsi"

&adc0_ad9680 {
	powerdown-gpios = <&sys_gpio_out 10 0>;
};

&dac0_ad9144 {
	txen-gpios = <&sys_gpio_out 9 0>;
	reset-gpios = <&sys_gpio_out 8 0>;
	irq-gpios = <&sys_gpio_in 2 0>;
};

&clk0_ad9523 {
	adi,pll2-m1-freq = <750000000>;
	sync-gpios = <&sys_gpio_out 6 0>;
	status0-gpios = <&sys_gpio_in 0 0>;
	status1-gpios = <&sys_gpio_in 1 0>;
};

&ad9523_0_c4 {
	adi,channel-divider = <3>;
};

&ad9523_0_c9 {
	adi,channel-divider = <3>;
};
